    "postinstall": "patch-package"
  },
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.js",
      "default": "./lib/index.js"
    },
    "./matrix-js-sdk": {
      "import": "./vendor/matrix-js-sdk/index.js",
      "require": "./vendor/matrix-js-sdk/index.js",
      "default": "./vendor/matrix-js-sdk/index.js"
    },
    "./matrix-js-sdk/*": {
      "import": "./vendor/matrix-js-sdk/*.js",
      "require": "./vendor/matrix-js-sdk/*.js",
      "default": "./vendor/matrix-js-sdk/*.js"
    }
